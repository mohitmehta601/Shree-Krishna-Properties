{
  "name": "shree-krishna-properties",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A production-ready property listing platform with separate user and admin dashboards",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "seed-admin": "npx tsx scripts/seed-admin.ts",
    "seed-sample": "npx tsx scripts/seed-sample-data.ts",
    "deploy:check": "npm run typecheck && npm run build",
    "deploy:windows": ".\\deploy.bat",
    "deploy:unix": "./deploy.sh"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "@types/react-router-dom": "^5.3.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.35",
    "supabase": "^2.51.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
